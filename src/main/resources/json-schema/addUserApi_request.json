{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "userName": {
      "type": "string",
      "minLength": 1
    },
    "gender": {
      "type": "string",
      "minLength": 1
    },
    "age": {
      "type": "integer",
      "minimum": 18
    },
    "mobileNo": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "userName",
    "gender",
    "age",
    "mobileNo"
  ],
  "type": "object"
}