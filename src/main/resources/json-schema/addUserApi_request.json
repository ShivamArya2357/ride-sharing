{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "userName": {
      "type": "string",
      "minLength": 1
    },
    "gender": {
      "type": "string",
      "minLength": 1
    },
    "age": {
      "type": "integer",
      "minimum": 18
    },
    "selectionStrategy": {
      "type": "string",
      "minLength": 1
    },
    "vehicle": {
      "type": "object",
      "properties": {
        "vehicleOwner": {
          "type": "string",
          "minLength": 1
        },
        "vehicleModel": {
          "type": "string",
          "minLength": 1
        },
        "vehicleNo": {
          "type": "string",
          "minLength": 1
        }
      }
    }
  },
  "required": [
    "userName",
    "gender",
    "age"
  ],
  "type": "object"
}